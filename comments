%{
#include <stdio.h>
%}

%%

"//"[a-zA-Z0-9]*        { printf("%s\n", yytext); }

"/*"[a-zA-Z0-9]*"*/"    { printf("%s\n", yytext); }

[a-zA-Z0-9]+            { /* words ignored */ }

[ \t\n]+                ;   /* skip whitespace */

.                       ;   /* skip any other character */

%%

int main()
{
    yylex();
    return 0;
}
